{
    "name": "dndroller",
    "version": "1.0.0",
    "description": "- commit subject lines should be 50 characters or less\r - begin with an emoji write them with a keyword wrapped in colons `:book:` :book:\r - followed by a verb in the imperative ie: add, remove, compile\r - ending with a reference to the relevant issue number with a hashtag and in parentheses ie: `(#24)`",
    "main": "index.js",
    "scripts": {
        "build": "parcel build index.html --public-url ./",
        "start": "parcel index.html"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/AndreBClark/dndroller.git"
    },
    "keywords": [],
    "author": "",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/AndreBClark/dndroller/issues"
    },
    "homepage": "https://github.com/AndreBClark/dndroller#readme",
    "alias": {
        "react": "preact/compat",
        "react-dom/test-utils": "preact/test-utils",
        "react-dom": "preact/compat"
    },
    "dependencies": {
        "@reach/router": "^1.2.1",
        "autoprefixer": "^9.7.3",
        "parcel-bundler": "^1.12.4",
        "postcss-cli": "^6.1.3",
        "postcss-modules": "^1.4.1",
        "preact": "^10.2.1"
    },
    "devDependencies": {
        "cssnano": "^4.1.10",
        "tailwind": "^4.0.0",
        "tailwindcss": "^1.1.4"
    }
}
